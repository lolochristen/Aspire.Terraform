{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "migrations": {
      "type": "project.v0",
      "path": "../MigrationService.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ConnectionStrings__db": "{db.connectionString}"
      }
    },
    "identityprovider": {
      "type": "project.v0",
      "path": "../IdentityProvider.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{identityprovider.bindings.http.targetPort}",
        "FrontEndUrl": "https://url",
        "Authentication__Authority": "https://url",
        "ConnectionStrings__db": "{db.connectionString}",
        "APPLICATIONINSIGHTS_CONNECTION_STRING": "{ai.connectionString}",
        "ConnectionStrings__secrets": "{secrets.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "apiservice": {
      "type": "project.v0",
      "path": "../ApiService.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{apiservice.bindings.http.targetPort}",
        "Authentication__Authority": "https://url",
        "ConnectionStrings__db": "{db.connectionString}",
        "ConnectionStrings__cache": "{cache.connectionString}",
        "APPLICATIONINSIGHTS_CONNECTION_STRING": "{ai.connectionString}",
        "ConnectionStrings__secrets": "{secrets.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "webfrontend": {
      "type": "project.v0",
      "path": "../.Web.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{webfrontend.bindings.http.targetPort}",
        "services__apiservice__http__0": "{apiservice.bindings.http.url}",
        "services__apiservice__https__0": "{apiservice.bindings.https.url}",
        "services__identityprovider__http__0": "{identityprovider.bindings.http.url}",
        "services__identityprovider__https__0": "{identityprovider.bindings.https.url}",
        "OpenIDConnectSettings__Authority": "https://url",
        "ConnectionStrings__db": "{db.connectionString}",
        "ConnectionStrings__cache": "{cache.connectionString}",
        "APPLICATIONINSIGHTS_CONNECTION_STRING": "{ai.connectionString}",
        "ConnectionStrings__secrets": "{secrets.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "cache": {
      "type": "azure.bicep.v0",
      "connectionString": "{cache.outputs.connectionString}",
      "path": "cache.module.bicep",
      "params": {
        "principalId": "",
        "principalName": ""
      }
    },
    "sqlserver": {
      "type": "azure.bicep.v0",
      "connectionString": "Server=tcp:{sqlserver.outputs.sqlServerFqdn},1433;Encrypt=True;Authentication=\u0022Active Directory Default\u0022",
      "path": "sqlserver.module.bicep",
      "params": {
        "principalId": "",
        "principalName": ""
      }
    },
    "db": {
      "type": "value.v0",
      "connectionString": "{sqlserver.connectionString};Database=verd.pay"
    },
    "ai": {
      "type": "azure.bicep.v0",
      "connectionString": "{ai.outputs.appInsightsConnectionString}",
      "path": "ai.module.bicep",
      "params": {
        "logAnalyticsWorkspaceId": ""
      }
    },
    "secrets": {
      "type": "azure.bicep.v0",
      "connectionString": "{secrets.outputs.vaultUri}",
      "path": "secrets.module.bicep",
      "params": {
        "principalId": "",
        "principalType": ""
      }
    }
  }
}